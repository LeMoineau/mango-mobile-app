{
  "name": "mango-mobile-app",
  "version": "2.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "predev": "npm run remove:shared && npm run replace:shared-import:dev",
    "prestart": "npm run remove:shared && npm run copy:shared",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "test": "jest",
    "prepare-build": "npm run copy:shared",
    "build:release": "npm run prepare-build && npm run eas:build:release",
    "build:apk": "npm run prepare-build && npm run eas:build:apk",
    "eas:build:release": "npx eas build -p android --profile production",
    "eas:build:apk": "npx eas build -p android --profile preview",
    "eas:build:web": "npx expo export -p web",
    "eas:inspect:release": "npx eas build:inspect --platform android --stage archive --output ~/release/archive --profile production",
    "publish": "npx eas submit --platform android",
    "copy:shared": "cpy ./../shared/src/**/* ./src/shared/src",
    "replace:shared-import:release": "replace '../../shared' 'shared' ./src -r",
    "replace:shared-import:dev": "replace 'shared/' '../../shared/' ./src -r",
    "remove:shared": "rimraf ./src/shared"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@shopify/flash-list": "^1.7.3",
    "axios": "^1.7.9",
    "base-64": "^1.0.0",
    "color-curves": "^0.0.7",
    "expo": "^52.0.40",
    "expo-asset": "~11.0.4",
    "expo-dev-client": "~5.0.14",
    "expo-linear-gradient": "~14.0.2",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "immer": "^10.1.1",
    "locale-codes": "^1.3.1",
    "locale-emoji": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-canvas": "^0.1.40",
    "react-native-circular-progress": "^1.4.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "6.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-triangle": "^0.0.9",
    "react-native-web": "~0.19.13",
    "socket.io-client": "^4.8.1",
    "use-elapsed-time": "^3.0.4",
    "use-immer": "^0.9.0",
    "zustand": "^4.5.6",
    "expo-navigation-bar": "~4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@types/base-64": "^1.0.2",
    "@types/react": "~18.3.12",
    "@types/react-native-canvas": "^0.1.14",
    "cpy-cli": "^5.0.0",
    "fs": "0.0.1-security",
    "react-native-svg-transformer": "^1.5.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.1.3"
  },
  "private": true
}
