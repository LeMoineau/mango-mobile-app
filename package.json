{
  "name": "mango-mobile-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "predev": "npm run remove:shared && npm run replace:shared-import:dev",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "test": "jest",
    "prepare-build": "npm run copy:shared && npm run replace:shared-import:release",
    "build:release": "npm run prepare-build && npm run eas:build:release",
    "build:apk": "npm run prepare-build && npm run eas:build:apk",
    "eas:build:release": "npx eas build -p android --profile production",
    "eas:build:apk": "npx eas build -p android --profile preview",
    "eas:build:web": "npx expo export -p web",
    "eas:inspect:release": "npx eas build:inspect --platform android --stage archive --output ~/release/archive --profile production",
    "publish": "npx eas submit --platform android",
    "copy:shared": "cpx ./../shared/src/**/* ./src/shared/src",
    "replace:shared-import:release": "replace '../../shared' 'shared' ./src -r",
    "replace:shared-import:dev": "replace 'shared/' '../../shared/' ./src -r",
    "remove:shared": "rimraf ./src/shared"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.14",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "@shopify/flash-list": "1.6.4",
    "axios": "^1.6.2",
    "base-64": "^1.0.0",
    "expo": "^51.0.8",
    "expo-file-system": "~17.0.1",
    "expo-linear-gradient": "~13.0.2",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-tab-view": "^3.5.2",
    "zustand": "^4.4.6",
    "expo-dev-client": "~4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.0.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "cpx": "^1.5.0",
    "renamer": "^5.0.0",
    "replace": "^1.2.2",
    "replace-in-files-cli": "^2.2.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "expo-modules-autolinking": "~1.11.0",
    "@expo/config-plugins": "~8.0.0"
  },
  "private": true
}
