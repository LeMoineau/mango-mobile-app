{
  "expo": {
    "name": "Mango",
    "slug": "mango-mobile-app",
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "backgroundColor": "#eee",
      "image": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#eee"
      },
      "package": "fr.pierrot.mango"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c87416f3-5ed0-42b8-b3cc-a202bff5fb16"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": ["expo-asset"]
  }
}
