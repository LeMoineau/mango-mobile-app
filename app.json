{
  "expo": {
    "name": "Mango",
    "slug": "mango-mobile-app",
    "version": "1.8.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#eee"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#eee"
      },
      "package": "fr.pierrot.mango"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c87416f3-5ed0-42b8-b3cc-a202bff5fb16"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#eee",
          "image": "./assets/favicon.png",
          "imageWidth": 100
        }
      ]
    ]
  }
}
